{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react-jsx",
		"module": "es6",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "../src/dist",
		"resolveJsonModule": true,
		"sourceMap": true,
		"target": "es5",
		"typeRoots": ["./node_modules/@types", "./src/utils"]
	},
	"include": ["src/**/*"]
}
